<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8.1" />
    </startup>
	<appSettings>
		<add key="BaseCadence" value="90" />
		
		<!-- basePowerValue:
		    generally set this to something close to sweet spot when not in TT mode.   
		    If you're in TT mode, set the average watts for the TT.  
		-->
		<add key="basePowerValue" value="165" />

		<!--  maxWattsAboveThreshold:
		    how much extra watts you want the bot to use when responding 
			to fluctuations in draft or grade. 		
		-->
		<add key="maxWattsAboveThreshold" value="50" />
		
		<!-- maxIdealTwentyMinuteAvgWatts:
		    the bot will make an attempt to avoid going over this 20 minute power 
		    by dropping power when you get close to this value.  The main use case is one
		    where you are trying to avoid catting up.  It isn't perfect, but it comes close. 
			
			It doesn't make sense to set this value lower than basePowerValue, 
			but it won't stop you from doing so.
		-->		
		<add key="maxIdealTwentyMinuteAvgWatts" value="180" />
		
		<!-- your zwift information -->
		<add key="zwiftUsername" value="" />
		<add key="zwiftPassword" value="" />
		<add key="primaryZwiftId" value="" />
		
	</appSettings>
